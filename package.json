{
  "name": "jupyterlab_kaitai",
  "version": "0.1.0",
  "description": "Experiment with Kaitai Struct in jupyterlab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/matthewturk/jupyterlab_kaitai",
  "bugs": {
    "url": "https://github.com/matthewturk/jupyterlab_kaitai/issues"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "Matthew Turk",
    "email": "matthewturk@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,.js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewturk/jupyterlab_kaitai.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm build:lib && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_kaitai/labextension",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepare": "jlpm clean && jlpm build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyter-widgets/base": "^4.0.0",
    "@jupyter-widgets/jupyterlab-manager": "^3.0.0",
    "@jupyterlab/application": "^3.0.0",
    "@jupyterlab/coreutils": "^5.0.2",
    "@jupyterlab/rendermime": "^3.0.6",
    "@jupyterlab/rendermime-interfaces": "^3.0.5",
    "@jupyterlab/services": "^6.0.3",
    "@jupyterlab/translation": "^3.0.5",
    "@types/dateformat": "^3.0.1",
    "@types/node": "^14.14.31",
    "big-integer": "^1.6.48",
    "dateformat": "^4.5.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.1.3"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "jupyterlab": {
    "extension": "lib/plugin.js",
    "outputDir": "jupyterlab_kaitai/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    },
    "discovery": {
      "kernel": [
        {
          "kernel_spec": {
            "language": "^python"
          },
          "base": {
            "name": "jupyterlab_kaitai"
          },
          "managers": [
            "pip"
          ]
        }
      ]
    }
  }
}
